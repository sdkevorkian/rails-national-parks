
<h1>New Park</h1>

<%=form_for @park do |p| %>
    <div class="form-chunk">
        <%=p.label :name%>
        <%=p.text_field :name%>

        <%=p.label :description%>
        <%=p.text_field :description%>

        <%=p.label :picture%>
        <%=p.text_field :picture%>
    </div>
    <div class="form-chunk">
      <%=p.collection_check_boxes :ranger_ids, @rangers, :id, :name %>
    </div>
    <div class="form-chunk">
      <%=p.submit%>
    </div>
<%end%>
